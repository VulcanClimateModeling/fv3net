#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-01-27/rnn-gscond-alltdep-c9af46/checkpoints/epoch.015.tf
tag=rnn-gscond-alltdep-c9af46-6hr
image=373b39524888bb784f488b6af81282de1cd3aed4

argo submit prog-run-and-eval.yaml \
    -p tag=$tag \
    -p image_tag=$image \
    -p config=$(base64 --wrap 0 ../configs/default_short.yaml) \
    -p tf_model=$url

