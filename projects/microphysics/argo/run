#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-02-17/precpd-gscond-combined/model.tf
tag=precpd-gscond-combined
image=38763c954da207088c1fbcf2681230e95c42c9f7

argo submit prog-run-and-eval.yaml \
    -p tag=$tag \
    -p image_tag=$image \
    -p config=$(base64 --wrap 0 ../configs/default.yaml) \
    -p tf_model=$url

