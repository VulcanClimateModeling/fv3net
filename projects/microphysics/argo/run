#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-01-12/rnn-v1-gpu-optimal-rnn-6a2cc6/model.tf
tag=rnn-v1-gpu-optimal-rnn-6a2cc6
