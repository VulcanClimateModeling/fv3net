#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-01-14/log-cloud-rnn-eps1e-8-4f3a28/model.tf
tag=log-cloud-rnn-eps1e-8-4f3a28
image=373b39524888bb784f488b6af81282de1cd3aed4

argo submit prog-run-and-eval.yaml \
    -p tag=$tag \
    -p image_tag=$image \
    -p config=$(base64 --wrap 0 ../configs/default.yaml) \
    -p tf_model=$url

