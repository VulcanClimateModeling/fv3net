#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-01-27/rnn-gscond-cloudtdep-cbfc4a/model.tf
tag=rnn-gscond-cloudtdep-cbfc4a
image=373b39524888bb784f488b6af81282de1cd3aed4

argo submit prog-run-and-eval.yaml \
    -p tag=$tag \
    -p image_tag=$image \
    -p config=$(base64 --wrap 0 ../configs/default.yaml) \
    -p tf_model=$url

