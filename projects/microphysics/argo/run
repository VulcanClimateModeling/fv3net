#!/bin/bash

url=gs://vcm-ml-experiments/microphysics-emulation/2022-01-27/rnn-gscond-cloudtdep-cbfc4a/model.tf
tag=online-24hr-cycle-v1
image=7f61afa0df2c9227c38b85473ac810a4cb36f367



argo submit prog-run-and-eval.yaml \
    --name "$tag" \
    -p image_tag="$image" \
    -p tag=$tag \
    -p config=$(base64 --wrap 0 ../configs/online.yaml) | grep -v 'config:' | tee -a submitted-jobs.txt
